<h1>New device</h1>

<% form_for(@device) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :deviceID %><br />
    <%= f.text_field :deviceID %>
  </p>
  <p>
    <%= f.label :deviceToken %><br />
    <%= f.text_field :deviceToken %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', devices_path %>